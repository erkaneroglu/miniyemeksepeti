@{
    ViewBag.Title = "Iletisim";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <title>Bootstrap E-commerce Templates</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <!--[if ie]><meta content='IE=8' http-equiv='X-UA-Compatible'/><![endif]-->
    <!-- bootstrap -->
    <link href="~/homepage-css/asd/bootstrap/css/bootstrap.min.css" rel="stylesheet">
    <link href="~/homepage-css/asd/bootstrap/css/bootstrap-responsive.min.css" rel="stylesheet">

    <link href="~/homepage-css/asd/themes/css/bootstrappage.css" rel="stylesheet" />

    <!-- global styles -->
    <link href="~/homepage-css/asd/themes/css/flexslider.css" rel="stylesheet" />
    <link href="~/homepage-css/asd/themes/css/main.css" rel="stylesheet" />

    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">

    <!-- scripts -->
    <script src="~/homepage-css/asd/themes/js/jquery-1.7.2.min.js"></script>
    <script src="~/homepage-css/asd/bootstrap/js/bootstrap.min.js"></script>
    <script src="~/homepage-css/asd/themes/js/superfish.js"></script>
    <script src="~/homepage-css/asd/themes/js/jquery.scrolltotop.js"></script>
    <style>
        a:hover {
            text-decoration: none;
            color: #ff6a00;
        }

        .center {
            display: block;
            margin-left: auto;
            margin-right: auto;
        }
    </style>
</head>

<body>
    <a href="/AnaSayfa/Index"><img src="~/Images/yemeklogo.png" style="margin-bottom:-30px;" class="center" /></a>
    <div id="top-bar" class="container">
        <div class="row">
            <div class="center">
                <ul class="user-menu">
                    <li><a href="~/AnaSayfa/Index/">Anasayfa</a></li>
                    <li><a href="~/AnaSayfa/SikSorulanSorular/">S.S.S</a></li>
                    <li><a href="~/AnaSayfa/Iletisim/">İletişim</a></li>
                    <li><a href="~/AnaSayfa/KullaniciSozlesmesi/">Kullanıcı Sözleşmesi</a></li>
                    <li><a><b> @User.Identity.Name </b></a></li>
                    @if (User.IsInRole("Admin"))
                    {
                        <li><a href="/Dashboard/Index/">Admin Paneli</a></li>
                    }
                    <li><a href="/Login/Logout/">Çıkış Yap</a></li>
                </ul>
            </div>
        </div>
    </div>
    <section class="py-5"></section>
    <div id="wrapper" class="container" style="margin-top:-40px">
        <section class="main-content">
            <div class="active item" style="margin-left:85px;margin-top:50px;padding:30px">
                <div style="text-align:center;font-weight:bold"><h4>İletişim</h4></div>
                <section class="google_map" style="margin-top:30px">
                    <iframe width="100%" height="400" frameborder="0" style="margin-left:-40px;margin-bottom:30px" scrolling="no" marginheight="0" marginwidth="0" src="http://maps.google.com/maps?f=q&amp;source=s_q&amp;hl=en&amp;geocode=&amp;q=74%2F6+Nguy%E1%BB%85n+V%C4%83n+Tho%E1%BA%A1i,+S%C6%A1n+Tr%C3%A0,+%C4%90%C3%A0+N%E1%BA%B5ng,+Vi%E1%BB%87t+Nam&amp;aq=0&amp;oq=74%2F6+Nguyen+Van+Thoai+Da+Nang,+Viet+Nam&amp;sll=37.0625,-95.677068&amp;sspn=41.546728,79.013672&amp;ie=UTF8&amp;hq=&amp;hnear=74+Nguy%E1%BB%85n+V%C4%83n+Tho%E1%BA%A1i,+Ng%C5%A9+H%C3%A0nh+S%C6%A1n,+Da+Nang,+Vietnam&amp;t=m&amp;ll=16.064537,108.24151&amp;spn=0.032992,0.039396&amp;z=14&amp;iwloc=A&amp;output=embed"></iframe>
                </section>
                <section class="main-content">
                    <div class="row">
                        <div class="center">
                                <h5>Merkez Ofis</h5>
                                <p>
                                    <strong>Telefon:</strong>&nbsp;(123) 456-7890<br>
                                    <strong>Fax:</strong>&nbsp;+04 (123) 456-7890<br>
                                    <strong>Email:</strong>&nbsp;<a href="#">info@yemeksepeti.com</a>
                                </p>
                                <br />
                                <h5>Müşteri Hizmetleri</h5>
                                <p>
                                    <strong>Telefon:</strong>&nbsp;(113) 023-1125<br>
                                    <strong>Fax:</strong>&nbsp;+04 (113) 023-1145<br>
                                    <strong>Email:</strong>&nbsp;<a href="#">destek@yahoo.com</a>
                                </p>
                        </div>
                    </div>
                </section>
            </div>
        </section>
        <section id="copyright">
            <span>Erkan Eroğlu 18701804 - Tüm hakları saklıdır.</span>
        </section>
    </div>


    <script src="~/homepage-css/asd/themes/js/common.js"></script>
    <script src="~/homepage-css/asd/themes/js/jquery.flexslider-min.js"></script>
    <script src="https://code.jquery.com/jquery-3.3.1.slim.min.js" integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo" crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.7/umd/popper.min.js" integrity="sha384-UO2eT0CpHqdSJQ6hJty5KVphtPhzWj9WO1clHTMGa3JDZwrnQq4sF86dIHNDz0W1" crossorigin="anonymous"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js" integrity="sha384-JjSmVgyd0p3pXB1rRibZUAYoIIy6OrQ6VrjIEaFf/nJGzIxFDsf4x0xIM+B07jRM" crossorigin="anonymous"></script>
    <script type="text/javascript">
        $(function () {
            $(document).ready(function () {
                $('.flexslider').flexslider({
                    animation: "fade",
                    slideshowSpeed: 4000,
                    animationSpeed: 600,
                    controlNav: false,
                    directionNav: true,
                    controlsContainer: ".flex-container" // the container that holds the flexslider
                });
            });
        });
    </script>
</body>
</html>


